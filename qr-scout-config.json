{
    "$schema": "../schema.json",
    "title": "QRScout",
    "page_title": "333 Scout Sheet - Default",
    "sections": [
        {
            "name": "Prematch",
            "fields": [
                {
                    "title": "Scouter Name",
                    "type": "text",
                    "required": true,
                    "code": "scouter",
                    "preserveDataOnReset": true,
                    "defaultValue": ""
                },
                {
                    "title": "Match # - Team (Format: TEAM#,MATCH#)",
                    "type": "text",
                    "required": true,
                    "code": "matchTeam",
                    "min": 3,
                    "max": 12,
                    "defaultValue": "",
                    "preserveDataOnReset": false
                },
                {
                    "title": "Robot Starting Position",
                    "type": "select",
                    "required": true,
                    "code": "robotPosition",
                    "choices": {
                        "0": "No show",
                        "1": "Subwoofer (amp side)",
                        "2": "Subwoofer (in front)",
                        "3": "Subwoofer (source side)",
                        "4": "Close to source"
                    },
                    "defaultValue": "2",
                    "preserveDataOnReset": false
                }
            ]
        },
        {
            "name": "Autonomous",
            "fields": [
                {
                    "title": "Leave?",
                    "type": "boolean",
                    "defaultValue": false,
                    "required": false,
                    "code": "autoLeave"
                },
                {
                    "code": "autoSpeaker",
                    "title": "Auto Speaker Scored",
                    "type": "counter",
                    "defaultValue": 0,
                    "min": 0,
                    "required": false
                },
                {
                    "code": "autoAmp",
                    "title": "Auto Amp Scored",
                    "type": "counter",
                    "defaultValue": 0,
                    "min": 0,
                    "required": false
                },
                {
                    "code": "autoWingIntake",
                    "title": "Wing Note Collected",
                    "type": "counter",
                    "defaultValue": 0,
                    "min": 0,
                    "max": 3,
                    "required": false
                },
                {
                    "code": "autoCenterIntake",
                    "title": "Center Note Collected",
                    "type": "counter",
                    "defaultValue": 0,
                    "min": 0,
                    "max": 5,
                    "required": false
                }
            ]
        },
        {
            "name": "Teleop",
            "fields": [
                {
                    "code": "teleopSpeaker",
                    "title": "Speaker Scored",
                    "type": "counter",
                    "defaultValue": 0,
                    "min": 0,
                    "required": false
                },
                {
                    "code": "teleopSpeakerMissed",
                    "title": "Speaker Missed",
                    "type": "counter",
                    "defaultValue": 0,
                    "min": 0,
                    "required": false
                },
                {
                    "code": "teleopAmp",
                    "title": "Amp Scored",
                    "type": "counter",
                    "defaultValue": 0,
                    "min": 0,
                    "required": false
                }
            ]
        },
        {
            "name": "Endgame",
            "fields": [
                {
                    "title": "Climb / Park",
                    "type": "select",
                    "required": true,
                    "code": "stagePos",
                    "choices": {
                        "NA": "No park",
                        "P": "Parked, climb not attempted",
                        "FC": "Failed climb",
                        "C1": "Climbed - alone",
                        "C2": "Climbed - 2 robot harmony",
                        "C3": "Climbed - 3 robot harmony"
                    },
                    "defaultValue": "NA"
                },
                {
                    "code": "stageTrap",
                    "title": "Notes in Trap",
                    "type": "select",
                    "choices": {
                        "NA": "No trap attempt",
                        "FT": "Failed trap",
                        "T1": "Trap (1 note)",
                        "T2": "Trap (2 notes)",
                        "T3": "Trap (3 notes)"
                    },
                    "defaultValue": "NA",
                    "required": false
                }
            ]
        },
        {
            "name": "Post-Match",
            "fields": [
                {
                    "code": "defenseReceived",
                    "title": "Defense Received",
                    "type": "select",
                    "choices": {
                        "0": "None",
                        "1": "Small amount of time",
                        "2": "Moderate amount of time",
                        "3": "Most of the match"
                    },
                    "required": false,
                    "defaultValue": "0"
                },
                {
                    "title": "Who played defense?",
                    "type": "select",
                    "required": false,
                    "code": "defenseNumber",
                    "choices": {
                        "20": "20 - The Rocketeers",
                        "229": "229 - Division By Zero",
                        "250": "250 - The Dynamos",
                        "296": "296 - Northern Knights",
                        "333": "333 - MEGALODONS",
                        "639": "639 - Code Red Robotics",
                        "1493": "1493 - Falcons",
